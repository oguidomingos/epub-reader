        if self.is_paused:
            mixer.music.unpause()
            self.is_paused = False
        else:
            mixer.music.play()
    def pause(self):
        if not self.is_paused:
            mixer.music.pause()
            self.is_paused = True
    def stop(self):
        mixer.music.stop()
        self.is_paused = False
        if os.path.exists(self.filename):
            os.remove(self.filename)
